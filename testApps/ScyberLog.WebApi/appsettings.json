{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ScyberLog": {
    "EnableConsole": false,
    "EnableFile": true,
    "FileNameTemplate": "Log\\{0:yyyy-MM-dd}\\log.txt",
    "FileFormatter": "json"
  }
}
